<dotnetnuke type="Package" version="5.0">
	<packages>
		<package name="R7.${PlatformType}.${PackType}.${Version}.${CultureCode}" type="${PackType}LanguagePack" version="${Version}">
			<friendlyName>${CultureNameNative}</friendlyName>
			<description />
			<iconFile />
			<owner>
				<name>Roman M. Yagodin</name>
				<organization>R7.Solutions</organization>
				<url>https://github.com/roman-yagodin/R7.DnnLocalization</url>
				<email>roman.yagodin@gmail.com</email>
			</owner>
			<license></license>
			<releaseNotes></releaseNotes>
			<!-- <license src="License.txt" />
			<releaseNotes src="ReleaseNotes.txt" /> -->
			<components>
				<component type="${PackType}Language">
					<languageFiles>
						<code>${CultureCode}</code>
						<displayName>${CultureNameNative}</displayName>
						<fallback>en-US</fallback>
						<languageFile>
							<path>${FilePath}</path>
							<name>${FileName}</name>
						</languageFile>
					</languageFiles>
				</component>
			</components>
		</package>
	</packages>
</dotnetnuke>
